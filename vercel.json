{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "framework": null,
  "rewrites": [
    { "source": "/api/(.*)", "destination": "https://ttprov5.onrender.com/api/$1" },
    { "source": "/(.*)", "destination": "/index.html" }
  ]
}
