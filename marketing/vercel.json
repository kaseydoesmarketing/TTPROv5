{
  "git": {
    "deploymentEnabled": {
      "TTPROv5": true,
      "bootstrap/v5": false,
      "main": false,
      "master": false
    }
  },
  "rewrites": [
    { "source": "/app", "destination": "/app" },
    { "source": "/app/(.*)", "destination": "/app/$1" }
  ],
  "env": {
    "NEXT_PUBLIC_API_BASE_URL": "@next_public_api_base_url"
  }
}
